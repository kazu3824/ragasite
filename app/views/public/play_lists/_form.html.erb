<%= form_with model: play_list, url: url, local:true do |f| %>
  <div class="form-group">
    <%= f.label :タイトル %>
    <%= f.text_field :title, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.collection_check_boxes(:track_ids, current_user.tracks, :id, :title) do |track|%>
      <%= track.check_box %>
      <%= track.object.title %>
    <% end %>
  </div>
  <div class="form-group">
    <%= f.submit "投稿する", class: 'btn btn-success' %>
  </div>
<% end %>