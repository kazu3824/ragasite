<%= @play_list.title %>
<%= render 'public/tracks/index', tracks: @tracks %>
<% if @play_list.user == current_user %>
  <%= link_to "編集する", edit_public_play_list_path(@play_list), class:"btn btn-success" %>
  <%= link_to "プレイリストを削除", public_play_list_path(@play_list), class: "btn btn-danger mr-1", method: :delete, "data-confirm" => "本当に削除しますか？" %>
<% end %>