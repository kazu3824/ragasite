<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th></th>
      <th>名前</th>
      <th>曲名</th>
      <th>歌手名</th>
      <th>いいね数</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% tracks.each do |track| %>
      <tr>
        <td><%= image_tag track.user.get_profile_image, size: '50x50' %></td>
        <td><%= track.user.name %></td>
        <td><%= link_to track.title,public_track_path(track.id), class: "track_#{track.id}" %></td>
        <td><%= track.artist.name %></td>
        <td><%= render "public/track_favorites/btn", track: track %></td>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>